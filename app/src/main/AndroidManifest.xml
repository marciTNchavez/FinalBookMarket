<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.paolo.ustbookmarketfinal" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomePage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Sell_Page"
            android:label="@string/title_activity_sell__page"
            android:parentActivityName=".HomePage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.paolo.ustbookmarketfinal.HomePage" />
        </activity>
        <activity
            android:name=".SearchPage"
            android:label="@string/title_activity_search_page"
            android:parentActivityName=".HomePage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.paolo.ustbookmarketfinal.HomePage" />
        </activity>
        <activity
            android:name=".SellResp"
            android:label="@string/title_activity_sell_resp"
            android:parentActivityName=".Sell_Page" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.paolo.ustbookmarketfinal.Sell_Page" />
        </activity>
    </application>

</manifest>
